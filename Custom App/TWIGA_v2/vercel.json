{
  "version": 2,
  "name": "twiga-v2",
  "framework": "vite",
  "rootDirectory": "./",
  "buildCommand": "npm run build:frontend",
  "outputDirectory": "dist/public",
  "installCommand": "npm ci",
  "functions": {},
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
